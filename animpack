for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.MainScreenGui:GetChildren()) do
	if v.Name == "AnimationPack" then
		v:Destroy();
	end
end
if not game.CoreGui:FindFirstChild("Animation") then
	local Animation = Instance.new("ScreenGui")
	local CLOSE = Instance.new("TextButton")
	local OPEN = Instance.new("TextButton")
	local ScrollingFrame = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")
	local Lean = Instance.new("TextButton")
	local Lay = Instance.new("TextButton")
	local Dance1 = Instance.new("TextButton")
	local Dance2 = Instance.new("TextButton")
	local Greet = Instance.new("TextButton")
	local ChestPump = Instance.new("TextButton")
	local Praying = Instance.new("TextButton")
	local Stop = Instance.new("TextButton")
	local UniversalAnimation = Instance.new("Animation");

	-- // Services
	local Players = game:GetService("Players")

	-- // Vars
	local Plr = Players.LocalPlayer
	local Char = Plr.Character
	local Humanoid = Plr.Character:FindFirstChildOfClass("Humanoid")

	-- Functions
	function StopAnimations()
		for i,v in pairs(Plr.Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks()) do
			if (v.Animation.AnimationId:match("rbxassetid")) then
				v:Stop();
			end
		end
	end
	function PlayAnimation(id)
		if UniversalAnimation.AnimationId == id then
			StopAnimations();
			UniversalAnimation.AnimationId = "1";
		else
			UniversalAnimation.AnimationId = id;
			local Plr = game:GetService("Players").LocalPlayer
			local AnimationTrack = Plr.Character.Humanoid:LoadAnimation(UniversalAnimation);
			AnimationTrack:Play();
		end
	end

	-- Properties
	Animation.Name = "Animation"
	Animation.Parent = game.CoreGui
	Animation.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	CLOSE.Name = "CLOSE"
	CLOSE.Parent = Animation
	CLOSE.BackgroundColor3 = Color3.fromRGB(255, 112, 112)
	CLOSE.ClipsDescendants = true
	CLOSE.Position = UDim2.new(0, 0, 0, 590)
	CLOSE.Size = UDim2.new(0, 120, 0, 29)
	CLOSE.Visible = false
	CLOSE.Font = Enum.Font.GothamBlack
	CLOSE.Text = "CLOSE"
	CLOSE.TextColor3 = Color3.fromRGB(0, 0, 0)
	CLOSE.TextSize = 20.000
	CLOSE.TextWrapped = true
	CLOSE.MouseButton1Click:Connect(function()
		ScrollingFrame.Visible = false
		CLOSE.Visible = false
		OPEN.Visible = true
	end)

	OPEN.Name = "OPEN"
	OPEN.Parent = Animation
	OPEN.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	OPEN.ClipsDescendants = true
	OPEN.Position = UDim2.new(0, 0, 0, 490)
	OPEN.Size = UDim2.new(0, 102, 0, 18)
	OPEN.Font = Enum.Font.SourceSansBold
	OPEN.Text = "Animations"
	OPEN.TextColor3 = Color3.fromRGB(0, 0, 0)
	OPEN.TextSize = 15.000
	OPEN.MouseButton1Click:Connect(function()
		ScrollingFrame.Visible = true
		CLOSE.Visible = true
		OPEN.Visible = false
	end)

	ScrollingFrame.Parent = Animation
	ScrollingFrame.Active = true
	ScrollingFrame.BackgroundColor3 = Color3.fromRGB(102, 102, 102)
	ScrollingFrame.Position = UDim2.new(0, 0, 0, 395)
	ScrollingFrame.Size = UDim2.new(0, 120, 0, 195)
	ScrollingFrame.Visible = false
	ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 255)

	-- Animation Pack Configurations
	ScrollingFrame.Visible = false
	ScrollingFrame.Active = true
	ScrollingFrame.Draggable = false

	UIListLayout.Parent = ScrollingFrame
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0, 2)

	Lean.Name = "Lean"
	Lean.Parent = ScrollingFrame
	Lean.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Lean.ClipsDescendants = true
	Lean.Position = UDim2.new(0, 0, -0.051282052, 0)
	Lean.Size = UDim2.new(0.941999972, 0, 0, 30)
	Lean.Font = Enum.Font.GothamBlack
	Lean.Text = "Lean"
	Lean.TextColor3 = Color3.fromRGB(0, 0, 0)
	Lean.TextSize = 12.000
	Lean.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3152375249");
	end);

	Lay.Name = "Lay"
	Lay.Parent = ScrollingFrame
	Lay.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Lay.ClipsDescendants = true
	Lay.Size = UDim2.new(0.941999972, 0, 0, 30)
	Lay.Font = Enum.Font.GothamBlack
	Lay.Text = "Lay"
	Lay.TextColor3 = Color3.fromRGB(0, 0, 0)
	Lay.TextSize = 12.000
	Lay.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3152378852")
	end);

	Dance1.Name = "Dance1"
	Dance1.Parent = ScrollingFrame
	Dance1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Dance1.ClipsDescendants = true
	Dance1.Position = UDim2.new(0, 0, 0.0923076943, 0)
	Dance1.Size = UDim2.new(0.941999972, 0, 0, 30)
	Dance1.Font = Enum.Font.GothamBlack
	Dance1.Text = "Dance1"
	Dance1.TextColor3 = Color3.fromRGB(0, 0, 0)
	Dance1.TextSize = 12.000
	Dance1.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3189773368");
	end);

	Dance2.Name = "Dance2"
	Dance2.Parent = ScrollingFrame
	Dance2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Dance2.ClipsDescendants = true
	Dance2.Size = UDim2.new(0.941999972, 0, 0, 30)
	Dance2.Font = Enum.Font.GothamBlack
	Dance2.Text = "Dance2"
	Dance2.TextColor3 = Color3.fromRGB(0, 0, 0)
	Dance2.TextSize = 12.000
	Dance2.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3189776546");
	end);

	Greet.Name = "Greet"
	Greet.Parent = ScrollingFrame
	Greet.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Greet.ClipsDescendants = true
	Greet.Size = UDim2.new(0.941999972, 0, 0, 30)
	Greet.Font = Enum.Font.GothamBlack
	Greet.Text = "Greet"
	Greet.TextColor3 = Color3.fromRGB(0, 0, 0)
	Greet.TextSize = 12.000
	Greet.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3189777795");
	end);

	ChestPump.Name = "ChestPump"
	ChestPump.Parent = ScrollingFrame
	ChestPump.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ChestPump.ClipsDescendants = true
	ChestPump.Size = UDim2.new(0.941999972, 0, 0, 30)
	ChestPump.Font = Enum.Font.GothamBlack
	ChestPump.Text = "ChestPump"
	ChestPump.TextColor3 = Color3.fromRGB(0, 0, 0)
	ChestPump.TextSize = 12.000
	ChestPump.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3189779152");
	end);

	Praying.Name = "Praying"
	Praying.Parent = ScrollingFrame
	Praying.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Praying.ClipsDescendants = true
	Praying.Size = UDim2.new(0.941999972, 0, 0, 30)
	Praying.Font = Enum.Font.GothamBlack
	Praying.Text = "Praying"
	Praying.TextColor3 = Color3.fromRGB(0, 0, 0)
	Praying.TextSize = 12.000
	Praying.MouseButton1Click:Connect(function()
		StopAnimations();
		PlayAnimation("rbxassetid://3487719500");
	end);

	Stop.Name = "Stop"
	Stop.Parent = ScrollingFrame
	Stop.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Stop.ClipsDescendants = true
	Stop.Size = UDim2.new(0.941999972, 0, 0, 30)
	Stop.Font = Enum.Font.GothamBlack
	Stop.Text = "Stop Animation"
	Stop.TextColor3 = Color3.fromRGB(0, 0, 0)
	Stop.TextSize = 12.000
	Stop.MouseButton1Click:Connect(function()
		StopAnimations();
	end);
end
